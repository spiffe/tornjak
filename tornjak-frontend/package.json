{
  "name": "tornjak-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@carbon/charts": "^0.41.80",
    "@carbon/charts-react": "^0.41.80",
    "@carbon/themes": "^10.38.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/data-grid": "^4.0.0-alpha.33",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@material-ui/x-grid": "^4.0.0-alpha.33",
    "@mui/material": "^5.2.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.20",
    "axios": "^0.21.4",
    "bootstrap": "^4.5.3",
    "carbon-components": "^10.36.0",
    "carbon-components-react": "^7.30.0",
    "carbon-icons": "^7.0.7",
    "chart.js": "^3.5.1",
    "d3": "^7.0.1",
    "d3-selection": "^3.0.0",
    "dotenv": "^8.2.0",
    "file-saver": "^2.0.5",
    "node-sass": "^6.0.1",
    "prop-types": "^15.6.0",
    "puppeteer": "^10.4.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "recharts": "^2.1.4",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "url-join": "^4.0.1",
    "web-vitals": "^0.2.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k -n COMPILER,NODEMON -c yellow,blue \"tsc -w\" \"nodemon -w dist -q dist/index.js\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@types/carbon__icons-react": "^10.31.2",
    "@types/carbon-components-react": "^7.44.1",
    "@types/file-saver": "^2.0.5",
    "@types/react-router-dom": "^5.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-jest": "^27.2.2",
    "check-prop-types": "^1.1.2",
    "concurrently": "^6.2.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "fetch-mock": "^9.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.2",
    "jest-dom": "^4.0.0",
    "jest-transform-stub": "^2.0.0",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.13",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)",
      "<rootDir>/node_modules/(?!(@carbon)/)",
      "/@carbon/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx|mjs)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "modules": "auto"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-runtime",
      "@babel/proposal-class-properties",
      "@babel/transform-regenerator",
      "@babel/plugin-transform-template-literals",
      "@babel/plugin-transform-modules-commonjs"
    ]
  }
}
